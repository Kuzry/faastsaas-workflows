{
  "name": "faastsaas",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:faastsaas-core": "turbo run build --filter=@faastsaas/core",
    "dev": "turbo run dev",
    "dev:workflows": "turbo run dev --filter=@faastsaas/workflows",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "prettier-check": "turbo run prettier-check",
    "husky-prepare": "husky"
  },
  "devDependencies": {
    "turbo": "^2.1.2",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
